    agent any 
    stages {
        stage('Build') {
            steps {
                script {
                    echo 'building..'
                }
            }
        }
        stage('Test') {
            steps {
                script {
                     echo 'Running tests...'
                }
            }
        }
        stage('Deploy') {
            steps {
                script {
                    echo 'Deploying application...'
                }
            }
        }
    }
}

